  "repository": {
    "type": "git",
    "url": "https://github.com/hkumls/hkumls-site"
  },
  "bugs": {
    "url": "https://github.com/hkumls/hkumls-site/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
